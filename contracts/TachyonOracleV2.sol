interface DataFeed{

    function getSymbol() external view returns(string memory);

    function getCurrency() external view returns(string memory);

    function getDecimals() external view returns(uint256);

    function getCreator() external view returns (address);

    function getLatestPrice() external view returns (uint256);

    function getTimestamps() external view returns (uint256[] memory);

    function getHistoricPrice(uint256 timestamp) external view returns (uint256);

    function getPairHash() external view returns(bytes32);
}

contract TachyonOracleV2 is OwnableUpgradeSafe  {
    using SafeMath for uint256;
    using AddressUpgradeSafe for address;

    bool private CONSTANT_PRICE;
    uint256 private BASE_PRICE;

    //V2 data
    DataFeed feed;
    DataFeed[] feeds;
    uint256[] percentages;
    uint256 private PERCENTAGE_BP; //100000 will be 1%

    function initialize(bool constantPrice, uint256 price) public initializer {
        CONSTANT_PRICE = constantPrice;
        BASE_PRICE = price;
        PERCENTAGE_BP = 10000000;
        __Context_init_unchained();
        __Ownable_init_unchained();
    }

    function getCurrentPrice() public view returns (uint256){
        if(CONSTANT_PRICE)
            return BASE_PRICE;
        else{
            //later logic goes here
            uint256 newPrice = BASE_PRICE;
            uint256 feedsLength = feeds.length;
            for (uint i = 0; i < feedsLength; i++) {
                //get all feed prices
                uint256 latestPrice = feeds[i].getLatestPrice();
                latestPrice = latestPrice.mul(percentages[i]).div(PERCENTAGE_BP);

                newPrice = newPrice.add(latestPrice);
            }
            return newPrice;
        }
    }

    function setBasePrice(uint256 price) external onlyOwner(){
        BASE_PRICE = price;
    }

    function changeConstantPrice(bool allowed) external onlyOwner(){
        CONSTANT_PRICE = allowed;
    }

    //V2 methods
    function setPercentageBP(uint256 bp) external onlyOwner(){
        PERCENTAGE_BP = bp;
    }

    function getPercentageBP() public view returns (uint256){
        return PERCENTAGE_BP;
    }

    function getBasePrice() public view returns (uint256){
        return BASE_PRICE;
    }

    function setFeeds(address[] memory feedAddrs, uint256[] memory _percentages) external onlyOwner(){
        delete feeds;
        delete percentages;

        uint256 addrsLength = feedAddrs.length;

        for (uint i = 0; i < addrsLength; i++) {
            feeds.push(DataFeed(feedAddrs[i]));
            percentages.push(_percentages[i]);
        }
    }
}
